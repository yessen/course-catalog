{
  "version": 2,
  "builds": [
    {
      "src": "cc_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.13" }
    },
    {
      "src": "cc_backend/install.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "cc_backend/wsgi.py"
    }
  ],
  {
      "installCommand": "bash install"
  }
  "outputDirectory": "/public"
}
